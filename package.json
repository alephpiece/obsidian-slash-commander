{
	"name": "slash-commander",
	"version": "0.2.16",
	"description": "Customizable slash commands for Obsidian.md",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "vite build -w -m development",
		"build": "vite build -m production",
		"build:check": "tsc -noEmit && vite build -m production",
		"version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
		"icons": "node scripts/update-icon-list.mjs",
		"lint": "eslint src/",
		"lint:fix": "eslint --fix src/",
		"format": "prettier --write \"src/**/*.{ts,tsx,scss,css,json}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,scss,css,json}\"",
		"typecheck": "tsc -noEmit",
		"clean": "rimraf main.js styles.css *.map"
	},
	"keywords": [],
	"author": "alephpiece",
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@tailwindcss/vite": "^4.0.14",
		"@types/babel__core": "^7.20.5",
		"@types/node": "^22.13.10",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"@vitejs/plugin-react": "^4.3.4",
		"builtin-modules": "^5.0.0",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"node-fetch": "^3.3.0",
		"obsidian": "latest",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"sass": "^1.69.0",
		"tailwindcss": "4.0.14",
		"tslib": "^2.4.1",
		"typescript": "^5.3.0",
		"typescript-eslint": "^8.26.1",
		"vite": "6.2.2"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@popperjs/core": "^2.11.8",
		"i18next": "24.2.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-i18next": "^15.4.1",
		"react-popper": "^2.3.0",
		"zustand": "^5.0.3"
	},
	"packageManager": "yarn@4.7.0"
}
