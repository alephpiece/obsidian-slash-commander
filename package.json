{
    "name": "slash-commander",
    "version": "0.3.7",
    "description": "Customizable slash commands for Obsidian.md",
    "main": "main.js",
    "type": "module",
    "scripts": {
        "dev": "vite build -w -m development",
        "build": "vite build -m production",
        "build:check": "yarn test && yarn typecheck",
        "version:patch": "node scripts/version-bump.mjs patch && git add package.json manifest.json versions.json",
        "version:minor": "node scripts/version-bump.mjs minor && git add package.json manifest.json versions.json",
        "version:major": "node scripts/version-bump.mjs major && git add package.json manifest.json versions.json",
        "icons": "node scripts/update-icon-list.mjs",
        "lint:check": "eslint src/",
        "lint": "eslint --fix src/",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,css,json}\"",
        "format": "prettier --write \"src/**/*.{ts,tsx,scss,css,json}\"",
        "typecheck": "tsc -noEmit -p tsconfig.json",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --silent --coverage",
        "clean": "rimraf main.js styles.css *.map"
    },
    "keywords": [],
    "author": "alephpiece",
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@tailwindcss/vite": "^4.1.8",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^22.15.29",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.5.0",
        "@vitest/coverage-v8": "^3.2.3",
        "builtin-modules": "^5.0.0",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "jsdom": "^26.1.0",
        "node-fetch": "^3.3.2",
        "obsidian": "latest",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "sass": "^1.89.1",
        "tailwindcss": "4.1.8",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.1",
        "vite": "6.3.5",
        "vitest": "^3.2.0"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@popperjs/core": "^2.11.8",
        "i18next": "25.2.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-i18next": "^15.5.2",
        "zustand": "^5.0.5"
    },
    "packageManager": "yarn@4.7.0"
}
